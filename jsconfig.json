{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "target": "es6",
    "checkJs": true,
    "baseUrl": ".",
    "paths": {
      "utils/*": ["src/utils/*"],
      "components/*": ["src/components/*"],
      "hooks/*": ["src/hooks/*"],
      "themes": ["src/themes"],
      "pages/*": ["src/pages/*"]

    }
  },
  "include": ["src/**/*"]
}
